{
	"name": "server",
	"version": "1.0.0",
	"engines": {
		"node": "14.17.0"
	},
	"type" : "commonjs",
	"description": "",
	"main": "index.ts",
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
		"dev": "npm-run-all dev:*",
		"dev:watch": "nodemon --watch '*/**.ts' -e ts --exec tsc",
		"dev:run": "nodemon --watch build/index.js --exec 'node build/index.js'",
		"test": "jest --coverage",
		"test:watch": "jest --watch --coverage",
		"start:prod": "node build",
		"start:dev": "nodemon",
		"build": "tsc",
		"lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
		"clear_jest": "jest --clearCache"
	},
	"author": "",
	"license": "ISC",
	"nodemonConfig": {
		"watch": [
			"./src"
		],
		"ext": "ts",
		"ignore": [
			"src/**/*.spec.ts",
			"src/**/__tests__/*",
			"src/node_modules/*"
		],
		"exec": "node --inspect -r ts-node/register -r dotenv/config -r tsconfig-paths/register ./index.ts",
		"inspect": true
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"js",
			"node"
		],
		"moduleNameMapper": {
			"src/(.*)": "<rootDir>/src/$1"
		},
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"(\\w|\\\\)*.d.ts"
		]
	},
	"dependencies": {
		"@types/dotenv": "^8.2.0",
		"@types/graphql": "^14.5.0",
		"aws-sdk": "latest",
		"axios": "latest",
		"body-parser": "latest",
		"chalk": "latest",
		"class-validator": "^0.13.1",
		"cors": "latest",
		"debug": "latest",
		"dotenv": "^8.2.0",
		"express": "latest",
		"express-graphql": "latest",
		"express-validator": "latest",
		"generate-schema": "^2.6.0",
		"graphql": "^15.5.0",
		"handlebars": "^4.7.7",
		"helmet": "latest",
		"helmet-csp": "latest",
		"htmlencode": "latest",
		"http-server": "^0.12.3",
		"if-env": "latest",
		"js-base64": "latest",
		"js-sha256": "latest",
		"lodash": "^4.17.21",
		"mailgun.js": "^3.3.2",
		"memory-cache": "latest",
		"moment": "latest",
		"moment-timezone": "latest",
		"morgan": "latest",
		"multer": "latest",
		"mustache": "latest",
		"nodemailer": "latest",
		"pg": "^8.6.0",
		"reflect-metadata": "latest",
		"sql-formatter": "latest",
		"stripe": "^8.47.0",
		"type-graphql": "^1.1.1",
		"typedi": "latest",
		"typeorm": "^0.2.32",
		"winston": "latest",
		"@prisma/client": "^2.23.0",
		"@types/express": "latest",
		"@types/graphql-fields": "^1.3.3",
		"@types/jest": "latest",
		"@types/lodash": "latest",
		"@types/mailgun-js": "latest",
		"@types/morgan": "latest",
		"@types/multer": "latest",
		"@types/mustache": "latest",
		"chai": "latest",
		"graphql-fields": "^2.0.3",
		"graphql-scalars": "^1.9.3",
		"graphql-tools": "latest",
		"jest": "latest",
		"mock-express": "^1.3.1",
		"node-mocks-http": "latest",
		"nodemon": "latest",
		"prettier": "latest",
		"prisma": "^2.23.0",
		"proxyquire": "latest",
		"sinon": "latest",
		"sinon-chai": "latest",
		"supertest": "latest",
		"ts-jest": "latest",
		"ts-node": "latest",
		"tsconfig-paths": "latest",
		"typegraphql-prisma": "^0.14.3",
		"typescript": "latest"
	},
	"devDependencies": {

	}
}
